{
  "name": "C.S.R. Sponsorkliks Extensie",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Wordt eraan herinnerd om relevante websites via Sponsorkliks te bezoeken",
  "homepage_url": "https://csrdelft.nl/",
  "icons": {
    "16": "icons/icon48.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "nl",
  "background": {
    "page": "src/bg/background.html"
  },
  "page_action": {
    "default_icon": "icons/icon48.png"
  },
  "permissions": [
    "webNavigation",
    "alarms",
    "notifications",
    "activeTab",
    "*://*.csrdelft.nl/API/*"
  ]
}
