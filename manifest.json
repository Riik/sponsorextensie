{
  "name": "C.S.R. Sponsorkliks Extensie",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Wordt eraan herinnerd om relevante websites via Sponsorkliks te bezoeken",
  "homepage_url": "https://csrdelft.nl/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/page_action/page_action.html"
  },
  "permissions": [
    "contentSettings",
    "http://*/*",
    "https://*/*"
  ]
}